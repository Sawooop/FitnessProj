<Page x:Class="FitnessProj.MainMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FitnessProj"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainMenu">

    <Grid Name="MenuGrid">

        <ProgressBar x:Name="LevelBar" Value="0" BorderThickness="25" Margin="275,286,275,105"></ProgressBar>
        <Button x:Name="tempDisplay" Click="TempDisplay_Click" Content="Display current logbook" HorizontalAlignment="Left" Margin="10,377,0,0" VerticalAlignment="Top" Width="159" Height="63"/>
        <TextBlock x:Name="textBlockLevel" FontSize="20" HorizontalAlignment="Left" Margin="275,90,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="63" Width="250"/>
        <TextBlock x:Name="textBlockProgress" FontSize="15" HorizontalAlignment="Left" Margin="275,364,0,0" TextWrapping="Wrap" Text="progress" VerticalAlignment="Top" Width="250" Height="50"/>
    </Grid>
</Page>
