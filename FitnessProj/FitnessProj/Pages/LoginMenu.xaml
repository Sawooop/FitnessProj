<Page x:Class="FitnessProj.LoginMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FitnessProj"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginMenu">

    <Grid>

        <Button x:Name="loginbutton" FontSize="20" Content="Log In" Height="75" Click="Loginbutton_Click" HorizontalAlignment="Left" Margin="302,280,0,0" VerticalAlignment="Top" Width="124"/>
        <TextBox x:Name="textBoxUsername" FontSize="30" HorizontalAlignment="Left" Height="75" Margin="78,160,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="250" TextChanged="TextBoxUsername_TextChanged"/>
        <TextBox x:Name="textBoxPassword" FontSize="30" HorizontalAlignment="Left" Height="75" Margin="402,160,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="250"/>

    </Grid>
</Page>
